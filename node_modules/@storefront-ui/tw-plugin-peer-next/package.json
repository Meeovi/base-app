{
  "name": "@storefront-ui/tw-plugin-peer-next",
  "description": "SFUI peer-next plugin for Tailwind",
  "version": "3.1.0",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    },
    "./*": "./*"
  },
  "source": "index.ts",
  "main": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev-only": "vite build --watch",
    "build:peer-next": "vite build"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.18",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^3.9.1"
  },
  "peerDependencies": {
    "tailwindcss": ">= 4.0.0-alpha.20 || >= 4.0.0-beta.1 || >= 4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://docs.storefrontui.io/v2/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuestorefront/storefront-ui.git",
    "directory": "packages/sfui/tw-plugin-peer-next"
  },
  "bugs": {
    "url": "https://github.com/vuestorefront/storefront-ui/issues"
  }
}