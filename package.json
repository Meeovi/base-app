{
   "name": "meeovi-monorepo",
   "version": "0.0.8",
   "type": "module",
   "description": "This is a base template for my projects.",
   "author": "Sebastian Hilton (https://www.sebastianhilton.com)",
   "homepage": "https://www.sebastianhilton.com",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Meeovi/base-app"
   },
   "bugs": {
      "url": "https://github.com/Meeovi/base-app/issues"
   },
   "license": "MIT",
   "private": true,
   "scripts": {
      "build": "vite build",
      "check-types": "tsc --noEmit",
      "dev": "nuxt dev --port 3011",
      "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}'",
      "generate": "nuxt generate",
      "preview": "nuxt preview",
      "dev:base": "npm --workspace apps/app-base run dev",
      "dev:social": "npm --workspace apps/app-social run dev"
   },
   "dependencies": {},
   "devDependencies": {
      "turbo": "^2.7.3"
   },
   "workspaces": [
      "apps/*",
      "layers/*"
   ],
   "packageManager": "npm@11.7.0"
}
